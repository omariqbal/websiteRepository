@model Omar_EAD_Term_Project.Models.dataClass
<!DOCTYPE html>
<html lang="en">
<head>
    <title>Client Settings</title>
    <script type="text/javascript" src="js/Myriad_Pro_600.font.js"></script>
    

</head>
<body id="page2">
    <!-- content -->
    <section id="content">
        <article class="col1">
            <h3>Hot Travel</h3>
            <div class="pad">
                <div class="wrapper under">
                    <figure class="left marg_right1"><img src="~/images/page1_img1.jpg" alt=""></figure>
                    <p class="pad_bot2"><strong>K2 </strong></p>
                    <p class="pad_bot2">One of the most beautifull place in Pakistan</p>
                    <a href="#" class="marker_1"></a>
                </div>
                <div class="wrapper under">
                    <figure class="left marg_right1"><img src="~/images/page1_img2.jpg" alt=""></figure>
                    <p class="pad_bot2"><strong>Gasherbrum III </strong></p>
                    <p class="pad_bot2">One of the most beautifull place in Pakistan</p>
                    <a href="#" class="marker_1"></a>
                </div>
                <div class="wrapper">
                    <figure class="left marg_right1"><img src="~/images/page1_img3.jpg" alt=""></figure>
                    <p class="pad_bot2"><strong>Nanga Parbat </strong></p>
                    <p class="pad_bot2">One of the most beautifull place in Pakistan</p>
                    <a href="#" class="marker_1"></a>
                </div>
            </div>
        </article>
        <article class="col2 pad_left1">

            <h2 align="center">CLIENT SETTINGS</h2>
            <h3 align="center"><label>Username:&nbsp;&nbsp;&nbsp;</label>@Model.u.username</h3>
            <form id="ContactForm" method="post" action="/client/modifyClient/@Model.u.Id">
                <div>
                    <div class="wrapper"><input type="text" class="input" name="name" value="@Model.c.name">Name:<br /></div>
                    <div class="wrapper"><input type="text" class="input " name="email" value="@Model.c.email">E-mail:<br /></div>
                    <div class="wrapper"><input type="text" class="input" name="address" value="@Model.c.address">Address:<br /></div>
                    <div class="wrapper"><input type="text" class="input" name="phno" value="@Model.c.phno">Phone #:<br /></div>
                    <div class="wrapper"><input type="text" class="input" name="pwd" value="@Model.u.pwd">Password:<br /></div>
                    @if(Session["t_id"]!=null)
                    {
                    <div class="wrapper"><label  name="tourID" >Tour ID: @Session["t_id"]</label><br /></div>
                    <div class="wrapper"><label  name="tourName"></label>Tour Name: @Session["t_name"]<br /></div>
                    }
                    <input type="submit" value="Edit" class="button" />
                </div>
            </form>
        </article>
    </section>
    <!-- / content -->
</body>
</html>